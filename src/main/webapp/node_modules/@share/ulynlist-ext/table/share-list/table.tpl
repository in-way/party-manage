<div class="share-list clearfix">

    {{ each list as item idx}}
    <div class="list-item">
        <div class="list-item-box clearfix">

            {{ each tableColumn.columns as column }}
            <div class="item clearfix" style="{{ column.style }}">
                {{if column.label}}<label>{{column.label}}：</label>{{/if}}
                <div class="sub-item">
                    <span class="textShow" overflowview="{{column.overflowview}}">
                        {{$outputContent item column currentObj idx}}
                    </span>
                </div>
            </div>
            {{ /each }}

        </div>
    </div>
    {{ /each }}

    {{if list.length === 0}}
    <p class="noData">暂无代办事项</p>
    {{/if}}
</div>