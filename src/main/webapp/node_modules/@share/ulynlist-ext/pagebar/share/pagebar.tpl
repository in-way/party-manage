<div class="share_table_nav clearfix">
    <ul class="pagebottonlist clearfix">
        <li class="clearfix">
            <span class="tableBarSpan">共有 {{opts.totalNum}} 条</span>
            <span
                    {{if opts.extra.linesPerPageEditable && opts.totalNum >=10}}
                        style="cursor:pointer;"
                    {{else}}
                        style="cursor:default;"
                    {{/if}}
                    class="tableBarSpan bg-white js-tableBarPageSize">
                每页 <em
                        {{if opts.extra.linesPerPageEditable && opts.totalNum >=10}}
                            style="cursor:pointer;"
                        {{else}}
                            style="cursor:default;"
                        {{/if}}
                        class="tableBarPageSize">{{opts.linesPerPage}}</em> 条
                <!--{{if opts.extra.linesPerPageEditable!=false}}-->
                    <!--每页<input type="text" value="{{opts.linesPerPage}}" class="setLinesPerPage showPerpage"/>条-->
                <!--{{else}}-->
                     <!--每页 <em class="tableBarPageSize">{{opts.linesPerPage}}</em> 条-->
                <!--{{/if}}-->

            </span>
            {{if opts.extra.linesPerPageEditable}}
                {{if opts.totalNum >=10}}
                <div class="page_size_bar">
                    {{if opts.totalNum >=10 && opts.totalNum <20}}
                    <span class="size_val">10</span>
                    {{else if opts.totalNum >=20 && opts.totalNum <50}}
                    <span class="size_val">10</span>
                    <span class="size_val">20</span>
                    {{else if opts.totalNum >=50 && opts.totalNum <100}}
                    <span class="size_val">10</span>
                    <span class="size_val">20</span>
                    <span class="size_val">50</span>
                    {{else}}
                    <span class="size_val">10</span>
                    <span class="size_val">20</span>
                    <span class="size_val">50</span>
                    <span class="size_val">100</span>
                    {{/if}}
                    <div class="quire_set_size">
                        <input type="text" value="">
                        <button type="button" class="set_page_size fa fa-arrow-circle-right"></button>
                    </div>
                </div>
                {{/if}}
            {{/if}}
        </li>
        <li class="first clearfix">
            <!--<a href="javascript:;" page="1"{{if opts.currentPage==1}}class="btnNo"{{/if}}></a>-->
            <span class="tableBarSpan tableBarA" page="1">首页</span>
        </li>
        <li class="active pre_page clearfix">
            <!--<a href="javascript:;" page="{{opts.currentPage-1}}"{{if opts.currentPage==1}}class="btnNo"{{/if}}></a>-->
            <span class="tableBarSpan tableBarA" page="{{opts.currentPage-1}}">上一页</span>
        </li>
        <li class="clearfix">
            <!--<input type="text" value=""/>-->
            <span class="goTopage_01 setGoToPage"></span>
            <div class="select_page_bar">
                <div class="page_num_bar">
                    {{each opts.pageList as item}}
                    <span class="page_num {{if item == opts.currentPage}} active{{/if}}">{{item}}</span>
                    {{/each}}
                </div>
                <div class="quire_set_val">
                    <input type="text" value="">
                    <button type="button" class="go_page fa fa-arrow-circle-right"></button>
                </div>
            </div>
        </li>
        <li class="next_page clearfix">
            <!--<a href="javascript:;" page="{{opts.currentPage+1}}"{{if opts.totalPage==opts.currentPage}}class="btnNo"{{/if}}></a>-->
            <span class="tableBarSpan tableBarA" page="{{opts.currentPage+1}}">下一页</span>
        </li>
        <li class="last_page clearfix">
            <!--<a href="javascript:;" page="{{opts.totalPage}}" {{if opts.totalPage==opts.currentPage}}class="btnNo"{{/if}} ></a>-->
            <span class="tableBarSpan tableBarA" page="{{opts.totalPage}}">尾页</span>
        </li>
    </ul>
</div>